{
  "name": "Skill_Exchange",
  "version": "1.0.0",
  "private": true,
  "description": "Skill_Exchange Platform - Full Stack Application",
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && node server.js",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\""
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["Skill_Exchange", "education", "learning", "fullstack"],
  "author": "Grupo-A",
  "license": "ISC"
}
